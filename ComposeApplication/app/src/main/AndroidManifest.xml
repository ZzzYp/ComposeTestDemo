<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hahaha.composeapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeApplication">
        <activity
            android:name=".EditTextDemoActivity"
            android:exported="false"
            android:theme="@style/Theme.ComposeApplication" />
        <activity
            android:name=".CustomComposeActivity"
            android:exported="false"
            android:theme="@style/Theme.ComposeApplication.NoActionBar" />
        <activity
            android:name=".NativeActivity"
            android:exported="false" />
        <activity
            android:name=".ViewDemoActivity"
            android:exported="false"
            android:theme="@style/Theme.ComposeApplication.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/Theme.ComposeApplication.NoActionBar" />
        <activity
            android:name=".ModifierDemoActivity"
            android:exported="false"
            android:theme="@style/Theme.ComposeApplication.NoActionBar" />
        <activity
            android:name=".ButtonDemoActivity"
            android:exported="false"
            android:theme="@style/Theme.ComposeApplication.NoActionBar" />
        <activity
            android:name=".RememberActivity"
            android:exported="false"
            android:theme="@style/Theme.ComposeApplication.NoActionBar" />

        <activity
            android:name=".NoStatusAndStatusUpAndOneWayDataFlowActivity"
            android:exported="false"
            android:theme="@style/Theme.ComposeApplication.NoActionBar" />

        <activity
            android:name=".MutableStateActivity"
            android:exported="false"
            android:theme="@style/Theme.ComposeApplication.NoActionBar" />
        <activity
            android:name=".ComposePerformanceDangerActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeApplication.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>